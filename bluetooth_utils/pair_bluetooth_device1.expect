set timeout 30
spawn bluetoothctl
expect "# "
send "agent off\r"
expect "Agent unregistered"
send "\r"

expect "# "
send "agent NoInputNoOutput\r"
expect "Agent registered"
send "\r"

expect "# "
send "default-agent\r"
expect "Default agent request successful"
send "\r"

expect "# "
send "discoverable on\r"

expect "Authorize "
send "yes\r"
send "exit\r"



